{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.3.0",
    "generators": {
      "v1.0": {
        "generatorName": "typescript-axios",
        "inputSpec": "http://backend:8000/openapi.json",
        "output": "./src/api",
        "templateDir": "./templates",
        "reservedWordsMappings": {
          "options": "options"
        }
      }
    }
  }
}
